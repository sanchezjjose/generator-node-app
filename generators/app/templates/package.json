{
  "name": "<%= answers['app:name'] %>",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "debug": "FORCE_HTTPS=true node --inspect bin/www",
    "start:dev": "FORCE_HTTPS=true nodemon ./bin/www & npm run watch",
    "stop": "killall node",
    "watch": "npm run watch-css & livereload ./public -e 'css' & npm run watch-react & npm run watch:js",
    "watch-css": "catw -c 'lessc -' './public/stylesheets/*.less' -o ./public/stylesheets/bundle.css -v",
    "watch-react": "babel --presets es2015,react --watch src/ --out-dir lib/",
    "watch:js": "bin/watchify.sh",
    "build:js": "bin/browserify.sh",
    "build": "npm run compile && npm run build:js",
    "compile": "babel --presets es2015,react -d lib/ src/",
    "lint": "eslint src/** src/*/* lib/** lib/*/*",
    "test": "multi='xunit=test-results.xml spec=-' mocha --recursive -R mocha-multi",
    "test:watch": "mocha --reporter spec --compilers js:babel-register --watch --reporter min",
    "cover": "node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- -R spec --compilers js:babel-register test/*",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "async": "^1.5.2",
    "aws-sdk": "~2.3.12",
    "babel-polyfill": "^6.9.0",
    "body-parser": "~1.12.4",
    "bootstrap": "^3.3.6",
    "chalk": "^1.1.3",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.12.4",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.13.0",
    "less": "~2.7.1",
    "morgan": "~1.5.3",
    "react": "15.2.0",
    "react-addons-css-transition-group": "15.2.0",
    "react-addons-test-utils": "15.2.0",
    "react-addons-update": "15.2.0",
    "react-bootstrap": "0.30.0",
    "react-dom": "15.2.0",
    "react-fontawesome": "1.1.0",
    "react-icons": "2.2.1",
    "request": "^2.72.0",
    "serve-favicon": "~2.2.1",
    "shelljs": "^0.7.0",
    "sinon": "^1.17.4",
    "underscore.string": "^3.3.4"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "babel-cli": "^6.2.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.9.0",
    "babelify": "^7.3.0",
    "bam-eslint-config": "^0.1.9",
    "browserify": "^13.0.1",
    "catw": "~0.2.0",
    "eslint": "^2.13.1",
    "mocha": "^2.5.3",
    "mocha-multi": "^0.9.0",
    "sinon": "^1.17.4",
    "uglify-js": "^2.6.4"
  }
}
