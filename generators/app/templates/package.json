{
  "name": "node-react-app",
  "version": "0.0.1",
  "scripts": {
    "debug": "FORCE_HTTPS=true node --inspect bin/www",
    "start": "nodemon bin/www",
    "start:dev": "FORCE_HTTPS=true nodemon bin/www & webpack-dev-server",
    "start:production": "NODE_ENV=production FORCE_HTTPS=true node bin/www",
    "watch": "webpack --progress --watch",
    "stop": "killall node",
    "lint": "eslint src/** src/*/* lib/** lib/*/*",
    "test": "multi='xunit=test-results.xml spec=-' mocha --recursive -R mocha-multi",
    "test:watch": "mocha --reporter spec --compilers js:babel-register --watch --reporter min",
    "cover": "node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- -R spec --compilers js:babel-register test/*",
    "postinstall": "npm run lint"
  },
  "dependencies": {
    "body-parser": "~1.12.4",
    "bootstrap": "^3.3.6",
    "chalk": "^1.1.3",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.12.4",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.13.0",
    "jquery": "^3.1.1",
    "morgan": "~1.5.3",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "request": "^2.72.0",
    "serve-favicon": "~2.2.1",
    "shelljs": "^0.7.0"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "babel": "^6.5.2",
    "babel-cli": "^6.2.0",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.9.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.13.1",
    "less-loader": "^2.2.3",
    "mocha": "^2.5.3",
    "mocha-multi": "^0.9.0",
    "postcss-loader": "^0.9.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.1",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^2.1.0-beta.0"
  }
}
